{
  "compileOnSave": true,
  "compilerOptions": {
    //**** WEB
    "target": "es5",
    "module": "system",
    "inlineSourceMap": true,
    "inlineSources": true,
    "importHelpers": true,

    //**** COMMON
    //"declaration": true,
    //"declarationDir": "../rw4-builds/web/",

    //"outDir": "../rw4-builds/web",
    //"outFile": "../rw4-builds/web/build.js",

    "baseUrl": ".",
    "paths": {
      "rw-router/*": [ "libs/rw-router/*" ],
      "rw-login/*": [ "libs/rw-login/*" ]
    },

    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    //"experimentalDecorators": true,
    "noImplicitAny": false,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable"
    ]
  },
  "exclude": [
    //*** native
    //"app-native",
    //"App.tsx",

    "jspm_packages/",
    "node_modules/"

  ]
}

